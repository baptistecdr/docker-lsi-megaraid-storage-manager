{
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "automerge": true
    },
    {
      "matchPackageNames": [
        "eclipse-temurin"
      ],
      "versioning": "regex:^(?<major>\\d+)u(?<minor>\\d+)-b(?<patch>\\d+)-jre$"
    }
  ]
}
